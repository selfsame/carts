-- pico8 tween lib - by github.com/selfsame
_ts = {}
function lerp(a,b,r) return a+(b-a)*r end
function pow(n) return function(v) return v^n end end
function wait(d,f) tween(0,nil,0,d,{f=function(o) f() end}) end

function tween(o, p, v, d, _)
  _ = _ or {}
  local t = {o=o,p=p,v=v,d=d,s=d}
  t.f = _.f
  t.l = _.l or lerp
  t.ei = _.ei or _.e
  t.eo = _.eo or _.e
  t.iv = p and o[p] or o
  add(_ts,t)
  return t
end

function update_tweens()
  local ts = {}
  for t in all(_ts) do
    if (t.o) then
      t.d -= 1
      local r = 1-t.d/t.s
      if (t.ei and t.eo) then r = r<0.5 and t.ei(r*2)/2 or 1-t.eo((1-r)*2)/2
      elseif t.ei then r = t.ei(r)
      elseif t.eo then r = 1-t.eo(1-r) end
      local z = t.l(t.iv, t.v, r)
      if t.p then t.o[t.p] = z  else t.o = z end
      if (t.d > 0) then add(ts,t) elseif t.f then t.f(t.o) end
    end
  end
  _ts = ts
end

-- demo

-- tween(object, prop_name, target_value, duration, options_table)
-- options are 
-- "e" dual in+out easing fn
-- "ei","eo" in,out easing fn
-- "f" tween callback (takes 1 argument - the object)
-- "l" lerping function

function ball(x,y,r,c)
  return {x=x,y=y,r=r,c=c}
end

function draw_ball(o)
  circfill(o.x,o.y,o.r,o.c)
end

function lerp_string(a,b,r)
  local mlen = max(#a, #b)
  local sidx = flr(lerp(0,mlen,r))
  return sub(b,1,sidx)..sub(a,sidx+1,#a)
end

function demo(val, str)
  tween(a, "x", val, 100)
  tween(b, "x", val, 100, {e=pow(3)})
  tween(c, "x", val, 100, {eo=pow(2)})
  tween(d, "x", val, 100, {ei=pow(4)})
  tween(thing, "string", str, 100, {l=lerp_string})
end

function _update60()
  update_tweens()
end

function _draw()
  cls()
  foreach({a,b,c,d},draw_ball)
  foreach(bouncers, draw_ball)
  print(thing.string,0,45,12)
end

a = ball(5,5,4,8)
b = ball(5,15,4,9)
c = ball(5,25,4,10)
d = ball(5,35,4,11)

thing = {string = ""}

bouncers = {}
for i=0,10 do
  add(bouncers, ball(64,123,3,3+i))
end


function run_demo()
  demo(120, "user customized")
  wait(100, function() 
    demo(5, "lerping function")
    wait(100, function() 
      run_demo()
    end)
  end)
end

-- chaining callbacks

function bounce(o)
  tween(o, "x", rnd(128), 40+rnd(20), {f=bounce})
  tween(o, "r", 8, 20, {ei=pow(2)})
  tween(o, "y", 70, 20, {eo=pow(2), f=
    function(o) 
      tween(o, "r",   4, 20, {ei=pow(2)}) 
      tween(o, "y", 123, 20, {ei=pow(2)}) 
    end})
end

run_demo()
foreach(bouncers,bounce)


